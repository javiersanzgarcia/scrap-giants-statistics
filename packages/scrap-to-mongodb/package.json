    "nodemon": "2.0.20",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "tscpaths": "0.0.9",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "mongoose": "6.8.3",
    "morgan": "1.10.0"
  }
}
